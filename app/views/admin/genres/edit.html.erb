<div class="container">
  <div class="row">
    <h5 style="background-color:#f0f0f0; width:200px; padding:0px 40px; margin:30px 0 30px 0">ジャンル編集</h5>
  </div>

  <div class="row">
    <div class="col-log-8">
    <%= form_with model:@genre, url:admin_genre_path, local:true do |f| %>
      <label>ジャンル名</label>
      <%=f.text_field :name %>
      <%=f.submit "変更を保存", class:"btn btn-success" %>
　　<% end %>
    </div>
  </div>
</div>
